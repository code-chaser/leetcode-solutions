class Solution {
public:
    string getPermutation(int n, int k) {
        string ans = "";
        for(int i = 1;i<=n;i++){
      // helper(n,k,ans+to_string(i),index+1);
           ans+= to_string(i);
        }
      // helper(n,k,ans,0);
      // return a;
      k--;
      while(k--){
        next_permutation(ans.begin(),ans.end());
      }
      return ans;

    }
};
